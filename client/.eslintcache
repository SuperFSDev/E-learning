nimport './Home.css'\nimport CourseCard from './../../shared/CourseCard/Course-card'\nimport Loader from '../../shared/Spinner/Loader'\n\nimport Hero from './Hero'\nimport Features from './Features'\nimport Banner from './Banner'\nimport Footer from '../../layout/Footer/Footer'\n\n\n\nconst Home = props => {\n  const coursesService = new CoursesService()\n\n  const [courses, setCourses] = useState(() => {\n    coursesService.getRandomCourses()\n      .then(response => setCourses(response.data))\n      .catch(() => {\n        props.history.push('/courses')\n        props.handleToast(true, 'An error has occurred, please try again later', 'red')\n      })\n  })\n\n  return (\n    <motion.div initial={{ opacity: 0 }} animate={{ opacity: 1 }} exit={{ opacity: 0 }}>\n      \n      <Hero title='Aspire for more' p1='Learning keeps you in the lead.' p2='Get in-demand skills to impress anyone.' />\n\n      {/* Carousel */}\n      <Container>\n        <section className=\"carousel-section mt-5\">\n          <h2 className=\"mt-5 mb-5 text-center \">Explore our schools to find your perfect program</h2>\n          {/* {courses ? \n            <Carousel className='carousel'>\n              {courses.map(elm => \n                <Carousel.Item key={elm._id}>\n                  <img\n                    src={elm.imageUrl}\n                    alt={elm.title}\n                  />\n                  <Carousel.Caption>\n                    <h3>{ elm.title }</h3>\n                    <p>{elm.lead}</p>\n                  </Carousel.Caption>\n                </Carousel.Item>\n              )}\n            </Carousel>\n            :\n            <Loader />} */}\n\n          {courses ?\n            <Carousel className='carousel'>\n\n              <Carousel.Item >\n                <Row>\n                  {[...courses].slice(0, 4).map(elm =>\n                    //console.log([...courses])\n                    <CourseCard key={elm._id} {...elm} />\n                  )}\n                </Row>\n              </Carousel.Item>\n              <Carousel.Item >\n                <Row>\n                  {[...courses].slice(4, 8).map(elm =>\n                    //console.log([...courses])\n                    <CourseCard key={elm._id} {...elm} />\n                  )}\n                </Row>\n              </Carousel.Item>\n            </Carousel>\n            : <Loader />\n          }\n\n        </section>\n\n        {/* Features */}\n        <Features />\n\n      </Container>\n      \n      <Banner title='Make the most of your online learning experience' text='Our teachers will help you learn while staying home.' />\n\n      {/* <Footer /> */}\n\n    </motion.div>\n  )\n}\n\nexport default Home","/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Profiles/UserProfile/UserProfile.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Course-form/Edit-Course-form.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/TeachersList/TeachersList.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Profiles/UserProfile/EditUserForm.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Profiles/TeacherProfile/TeacherProfile.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Profiles/TeacherProfile/Edit-Teacher-Form.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Profiles/TeacherProfile/Create-Teacher-form.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/shared/Alert/Alert.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Course-details/Course-details.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/s