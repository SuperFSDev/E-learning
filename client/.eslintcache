sers/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Course-details/Course-details.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/TeachersList/TeacherDetails.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/TeachersList/TeachersList.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Signup/Signup.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Profiles/UserProfile/UserProfile.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Course-form/Edit-Course-form.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Profiles/UserProfile/EditUserForm.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Profiles/TeacherProfile/Create-Teacher-form.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Profiles/TeacherProfile/Edit-Teacher-Form.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Course-form/New-Course-form.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Profiles/TeacherProfile/TeacherProfile.js",["119"],"import { Container, Image, Col, Row, Button } from 'react-bootstrap'\nimport { Link } from 'react-router-dom'\nimport CoursesServices from '../../../../service/courses.service'\nimport TeachersServices from '../../../../service/teachers.service'\nimport React, { Component } from 'react'\nimport CourseCard from '../../../shared/CourseCard/Course-card'\nimport Loader from './../../../shared/Spinner/Loader'\nimport './TeacherProfile.css'\n\nclass TeacherProfile extends Component {\n  constructor() {\n    super()\n    this.state = {\n      courses: undefined\n    }\n    this.teachersServices = new TeachersServices()\n    this.coursesServices = new CoursesServices()\n  }\n\n  componentDidMount = () => this.refreshCourses()\n\n  refreshCourses = () => {\n    this.coursesServices\n      .getTeacherCourses(this.props.teacherInfo._id)\n      .then(response => this.setState({ courses: response.data }))\n      .catch(() => {\n        this.props.history.push('/profile')   //  TO-DO -- ¿está bien así?\n        this.props.handleToast(true, 'An error has occurred, please try again later', 'red')\n      })\n  }\n\n  deleteCourse = course_Id => {\n    this.coursesServices\n      .deleteCourse(course_Id)\n      .then(() => {\n        this.refreshCourses()\n        this.props.handleToast(true, 'Delete successful!', 'green')\n      })\n      .catch(() => this.props.handleToast(true, 'An error has occurred while deleting, please try again later', 'red')) //  TO-DO -- ¿está bien así?\n  }\n\n  deleteTeacher = () => {\n    const teacher_Id = this.props.teacherInfo._id\n    \n    !this.state.courses\n      ?\n      this.teachersServices\n        .deleteTeacher(teacher_Id)\n        .then(() => {\n          this.props.storeUser(this.props.loggedUser)\n          this.props.history.push('/profile')\n          this.props.handleToast(true, 'Delete successful!', 'green')\n        })\n        .catch(() => this.props.handleToast(true, 'An error has occurred while deleting, please try again later', 'red')) //  TO-DO -- ¿está bien así?\n      :\n      \n      this.coursesServices\n        .deleteTeacherCourses(teacher_Id)\n        .then(() => this.teachersServices.deleteTeacher(teacher_Id))\n        .then(() => {\n          this.props.storeUser(this.props.loggedUser)\n          this.props.history.push('/profile')\n          this.props.handleToast(true, 'Delete successful!', 'green')\n        })\n        .catch(() => this.props.handleToast(true, 'An error has occurred while deleting, please try again later', 'red')) //  TO-DO -- ¿está bien así?\n  }\n\n  render() {\n    return (\n      <>\n        <Container className=\"teacher-profile\">\n          <Row>\n\n            <Col md={{ span: 9 }}>\n              <p className=\"instructor\" style={{ color: '#73726c' }}>INSTRUCTOR</p>\n              <h1>{this.props.teacherInfo.name} {this.props.teacherInfo.surname} </h1>\n              <p><strong>{this.props.teacherInfo.jobOccupation}</strong></p>\n              <hr></hr>\n              <h3><strong>About me</strong></h3>\n              <p>{this.props.teacherInfo.description}</p>\n            </Col>\n            <Col md={{ span: 2, offset: 1 }} className=\"d-flex align-items-center flex-column\">\n              <Row>\n                <Image src={this.props.loggedUser.imageUrl} className=\"user-img mb-3\" roundedCircle alt={this.props.teacherInfo.name} />\n              </Row>\n              <Row className=\"mb-5\">\n                {/* \n              {this.props.teacherInfo.url ?\n                this.props.teacherInfo.url.map(elm => <a className=\"btn btn-success\" href={this.props.teacherInfo.url} target=\"_blank\" key={elm._id}{...elm}>Linkedin</a>)\n                : null\n              } */}\n                {this.props.teacherInfo.linkedin ?\n                  <a className=\"btn btn-outline-primary btn-block linkedin\" href={this.props.teacherInfo.linkedin} target=\"_blank\">Linkedin</a>\n                  : null\n                }\n              </Row>\n              <Row >\n                <Link to='/profile-teacher/edit-teacher' className=\"btn btn-info btn-block\">Edit teacher details</Link>\n                <Button onClick={this.deleteTeacher} className=\"btn btn-danger btn-block\">Delete teacher</Button>\n                <Link to='/profile-teacher/create-course' className=\"btn btn-success btn-block\">Create new course</Link>\n              </Row>\n            </Col>\n\n          </Row>\n\n          <hr></hr>\n\n          <Row>\n            <Col md={12}>\n              <h2 className=\"mt-5 mb-5\">Your Courses</h2>\n            </Col>\n          </Row>\n          <Row>\n            {\n              this.state.courses\n                ?\n                this.state.courses.map(elm => <CourseCard key={elm._id} {...elm} teacher={this.props.teacherInfo} userInfo={this.props.loggedUser} deleteCourse={this.deleteCourse} />)\n                :\n                <Loader />\n            }\n\n          </Row>\n        </Container>\n      </>\n    )\n  }\n}\n\n\nexport default TeacherProfile","/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/shared/Alert/Alert.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Courses-list/Courses-list.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/service/courses.service.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/service/upload.service.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/service/users.service.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/TeachersList/TeacherCard.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Login-form/LoginForm.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/shared/Spinner/Loader.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/shared/CourseCard/Course-card.js",["120"],"import { Link } from 'react-router-dom'\nimport { Col, Card, Button } from 'react-bootstrap'\nimport './Course-card.css'\nimport mine from './mine.ico'\nimport FavButton from '../../shared/FavButton/FavButton'\n\nconst CourseCard = props => {\n    return (\n        <Col lg={4}>\n            <Card className=\"course-card\">\n                <Card.Img variant=\"top\" src={props.imageUrl} alt={props.title} />\n                <Card.Body>\n                    <Card.Title>{props.title}</Card.Title>\n                    {/* TO-DO props.owner borrar despues de eliminar cursos sin owner*/}\n                    {props.owner ? <Card.Subtitle className=\"mb-2 text-muted\">by {props.owner.name} {props.owner.surname}</Card.Subtitle> : null}\n\n                    <Card.Text className=\"details\"><strong>Category:</strong> {props.category} | <strong>Level:</strong> {props.difficultyLevel} </Card.Text>\n                    <Card.Text><strong>Price:</strong> {props.price} € | <strong>Duration:</strong> {props.duration} h</Card.Text>\n\n                    <Link className=\"btn btn-dark\" to={`/courses/${props._id}`}>View more</Link>\n\n                    {props.teacher && props.owner === props.teacher._id\n                        ?\n                        <>\n                            <Link to={`/profile-teacher/edit-course/${props._id}`} className=\"btn btn-info mx-2\">Edit</Link>\n                            <Button to={`/profile-teacher/delete-course/${props._id}`} onClick={() => props.deleteCourse(props._id)} className=\"btn btn-danger\">Delete</Button>\n                            {/* <span><img src={mine} style={{ width: 20, height: 20 }} alt={props.title} /></span> */}\n                        </>\n                        : null\n                    }\n                    {props.userInfo ?\n                        <FavButton updateFavs={props.updateFavs} userInfo={props.userInfo} itemInfo={props} />\n                        : null\n                    }\n\n                </Card.Body>\n            </Card>\n        </Col>\n    )\n}\n\nexport default CourseCard","/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/shared/SearchBar/SearchBar.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/shared/FavButton/FavButton.js",[],{"ruleId":"121","replacedBy":"122"},{"ruleId":"123","replacedBy":"124"},{"ruleId