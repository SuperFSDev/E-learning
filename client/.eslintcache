e.course.videos.map(elm =>\n                                            <Card.Header className=\"video-card\" key={elm._id}>\n                                                <img src=\"https://res.cloudinary.com/dodneiokm/image/upload/v1607893554/project3-ironhack/play_u6mma0.png\" alt=\"play icon\" />\n                                                <a href={elm} target=\"_blank\" >Video {elm._id}</a>\n                                            </Card.Header>\n                                        )\n                                        : null\n                                    }\n                                </Col>\n                            </Row>\n                        </section>\n                        <Link to=\"/courses\" className=\"btn btn-sm btn-outline-dark mt-5\">Go back</Link>\n                    </>\n                    :\n                    <Loader />\n                }\n\n            </Container>\n        )\n    }\n}\n\nexport default CourseDetails\n\n\n","/home/marisa/Desktop/ironhack/project-3/client/src/components/pages/Signup/Signup.js",[],"/home/marisa/Desktop/ironhack/project-3/client/src/components/pages/Home/Home.js",["134","135"],"import { Container, Card, Col, Row, Carousel } from 'react-bootstrap'\nimport { Link } from 'react-router-dom'\nimport './Home.css'\nimport CourseCard from './../../shared/CourseCard/Course-card'\n\nconst Home = () => {\n  return (\n    <>\n      {/* Hero */}\n      <section className=\"hero \">\n        <Container fluid>\n          <Container>\n            <h1>Aspire for more</h1>\n            <p className=\"mb-0\"><em>Learning keeps you in the lead.</em></p>\n            <p><em>Get in-demand skills to impress anyone.</em></p>\n          </Container>\n        </Container>\n      </section>\n      {/* Carousel */}\n      <Container>\n        <section className=\"features text-center mt-5\">\n          <h2 className=\"mt-5 mb-5\">Explore our schools to find your perfect program</h2>\n          <Car