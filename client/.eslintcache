m.Label>Surname</Form.Label>\n                  <Form.Control\n                    required\n                    type='text'\n                    name='surname'\n                    placeholder='Alvarez'\n                    value={this.state.surname}\n                    onChange={this.handleInputChange} />\n                </Form.Group>\n              </Form.Row>\n\n              <Form.Row>\n                <Form.Group as={Col} lg='5' controlId='username'>\n                    <Form.Label>Username</Form.Label>\n                  <Form.Control\n                    required\n                    type='text'\n                    name='username'\n                    placeholder='popinez'\n                    value={this.state.username}\n                    onChange={this.handleInputChange} />\n                </Form.Group>\n\n                <Form.Group as={Col} lg='7' controlId='password'>\n                    <Form.Label>Password</Form.Label>\n                  <Form.Control\n                    required\n                    type='password'\n                    name='password'\n                    placeholder='fantasía caribeña'\n                    value={this.state.password}\n                    onChange={this.handleInputChange} />\n                  <Form.Text id='passwordHelpBlock' muted>\n                    Your password must be 8-10 characters long\n                  </Form.Text>\n                </Form.Group>\n              </Form.Row>\n\n              <Form.Row>\n                <Form.Group as={Col} lg='7' controlId='email'>\n                    <Form.Label>Email address</Form.Label>\n                  <Form.Control\n                    required\n                    type='email'\n                    name='email'\n                    placeholder='pop@ino.dog'\n                    value={this.state.email}\n                    onChange={this.handleInputChange} />\n                </Form.Group>\n\n                <Form.Group as={Col} lg='5' controlId='role'>\n  