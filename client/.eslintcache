      onChange={this.handleInputChange} />\n                </Form.Group>\n\n                <Form.Group as={Col} lg='7' controlId='surname'>\n                    <Form.Label>Surname</Form.Label>\n                  <Form.Control\n                    required\n                    type='text'\n                    name='surname'\n                    placeholder='Alvarez'\n                    value={this.state.surname}\n                    onChange={this.handleInputChange} />\n                </Form.Group>\n              </Form.Row>\n\n              <Form.Row>\n                <Form.Group as={Col} lg='5' controlId='username'>\n                    <Form.Label>Username</Form.Label>\n                  <Form.Control\n                    required\n                    type='text'\n                    name='username'\n                    placeholder='popinez'\n                    value={this.state.username}\n                    onChange={this.handleInputChange} />\n                </Form.Group>\n\n                <Form.Group as={Col} lg='7' controlId='password'>\n                    <Form.Label>Password</Form.Label>\n                  <Form.Control\n                    required\n                    type='password'\n                    name='password'\n                    placeholder='fantasía caribeña'\n                    value={this.state.password}\n                    onChange={this.handleInputChange} />\n                  <Form.Text id='passwordHelpBlock' muted>\n                    Your password must be 8-10 characters long\n                  </Form.Text>\n                </Form.Group>\n              </Form.Row>\n\n              <Form.Row>\n                <Form.Group as={Col} lg='7' controlId='email'>\n                    <Form.Label>Email address</Form.Label>\n                  <Form.Control\n                    required\n                    type='email'\n                    name='email'\n                    placeholder='pop@ino.dog'\n                    value={this.state.email}\n                    onChange={this.handleInputChange} />\n                </Form.Group>\n\n                <Form.Group as={Col} lg='5' controlId='role'>\n                  <Form.Label>Choose role</Form.Label>\n                  <Form.Control as='select' name='role' value={this.state.role} onChange={this.handleInputChange}>\n                    <option>Student or Teacher?</option>\n                    <option value='Student' >Student</option>\n                    <option value='Teacher' >Teacher</option>\n                  </Form.Control>\n                </Form.Group>\n              </Form.Row>\n\n              <Form.Group>\n                <Button variant='dark' type='submit'>Let's started !</Button>\n              </Form.Group>\n            </Form>\n          </Col>\n        </Row>\n      </Container>\n    )\n  }\n}\n\nexport default Signup","/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Course-details/Course-details.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Courses-list/Courses-list.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Profiles/UserProfile/UserProfile.js",[],"/Users/Jose/Desktop/IRONHACK/BOOTCAMP/PROYECTO-FINAL/v1/project-3/client/src/components/pages/Profiles/TeacherProfile/TeacherProfile.js",["82"],"import { Container } from 'react-bootstrap'\nimport { Link } from 'react-router-dom'\nimport TeacherCard from './Teacher-Card'\nimport TeacherServices from './../../../../service/teachers.service'\nimport React, { Component } from 'react'\n\nclass TeacherProfile extends Component {\n  constructor() {\n    super()\n    this.state = {\n      teacher: undefined\n    }\n    this.teacherServices = new TeacherServices()\n  }\n\n  componentDidMount = () => {\n    this.teacherServices\n      .getTeacher(this.props.loggedUser._id)\n      .then(response => this.setState({ teacher: response.data[